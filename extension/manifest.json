{
	"name": "UARadio",
	"description": "Українські радіостанції",
	"version": "0.1.0",
	"manifest_version": 2,
	"default_locale": "uk",
	"icons": {
		"16": "assets/icon16.png",
		"48": "assets/icon48.png",
		"128": "assets/icon128.png"
	},
	"permissions": ["storage"],
	"background": {
	    "scripts": ["background.js"],
	    "persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"options_page": "options.html"
}